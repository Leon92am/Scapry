<script type="text/javascript">
$(document).ready(function(e) {			
	var ttop = $('.fixad').offset().top;
    var mhtop = $('.maincol').top;
	var mh = $('.maincol').height();
	var fh = $('.fixad').height();
	$(window).scroll(function(e){
		var sdtop = $(document).scrollTop();	
		if(sdtop > ttop - 10){
			$('.fixad').css('position','fixed');
			if(sdtop + fh > mh){
				$('.fixad').css('top',mh-sdtop-fh+'px');	
			}				
		}else{
			$('.fixad').css('position','');
		}
	})
});
